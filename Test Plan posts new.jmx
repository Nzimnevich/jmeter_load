        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler" enabled="true">
          <stringProp name="BeanShellSampler.query">// Простейшая версия
try {
    String userIds = vars.get(&quot;userIds&quot;);
    log.info(&quot;userIds = &quot; + userIds);
    
    if (userIds == null) {
        throw new Exception(&quot;userIds is null - check JSON Extractor&quot;);
    }
    
    String[] userIdsArray = userIds.split(&quot;,&quot;);
    int randomIndex = (int)(Math.random() * userIdsArray.length);
    String randomUserId = userIdsArray[randomIndex].trim();
    
    vars.put(&quot;currentUserId&quot;, randomUserId);
    props.put(&quot;userIdsString&quot;, userIds);
    
    log.info(&quot;Selected user: &quot; + randomUserId + &quot; from &quot; + userIdsArray.length + &quot; users&quot;);
    
} catch (Exception e) {
    log.error(&quot;Error: &quot; + e.toString());
    throw e;
}


</stringProp>
        </BeanShellSampler>
